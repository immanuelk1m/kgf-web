{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2017", "dom"],
    "moduleResolution": "node",
    "outDir": "./netlify/functions",
    "rootDir": "./netlify/functions",
    "sourceMap": true
  },
  "include": ["netlify/functions/**/*"],
  "exclude": ["node_modules"]
}